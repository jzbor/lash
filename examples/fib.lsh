@usestd
@set strategy normal
@set numerals true

iszero := λn.n (λx. FALSE) TRUE;

fibh := \f x. IFTHENELSE
		(iszero x)
		(\f x . x)
		(IFTHENELSE
			(iszero (PRED x))
			(\f x . f x)
			(ADD (f f (PRED x)) (f f (PRED (PRED x)))));
fib := fibh fibh;

@echo ""
@echo "6. Fibonacci number"
listunsorted := !dbg (!cn (fib $6));
